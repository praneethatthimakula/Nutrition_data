{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-Nutrition-data-dev"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"nutrition_sp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'nutrition_sp'"
		},
		"ls_abs_devtest_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "adb-nutrition-data-con-string"
		},
		"ls_akv_nutrition_demo_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().lprm_akv_url}"
		},
		"tr_nutrition_data_properties_copy data nutrition_parameters_prm_container_name": {
			"type": "string",
			"defaultValue": "nutrition-demo"
		},
		"default_properties_gprm_akv_url_value": {
			"type": "string",
			"defaultValue": "https://akv-nutrition-devtest.vault.azure.net/"
		},
		"default_properties_gprm_file_name_value": {
			"type": "string",
			"defaultValue": "copy_Nutrition_data3.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-Nutrition-data-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"nutrition_sp_connectionString": {
						"value": "[parameters('nutrition_sp_connectionString')]"
					},
					"default_properties_gprm_akv_url_value": {
						"value": "[parameters('default_properties_gprm_akv_url_value')]"
					},
					"default_properties_gprm_file_name_value": {
						"value": "[parameters('default_properties_gprm_file_name_value')]"
					},
					"ls_akv_nutrition_demo_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_akv_nutrition_demo_properties_typeProperties_baseUrl')]"
					},
					"ls_abs_devtest_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_abs_devtest_properties_typeProperties_connectionString_secretName')]"
					},
					"tr_nutrition_data_properties_copy data nutrition_parameters_prm_container_name": {
						"value": "[parameters('tr_nutrition_data_properties_copy data nutrition_parameters_prm_container_name')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}